<script>
window.onload = function() {
    try {
        $('#innerCal');
        colorbox();
        adjs();
    } catch (e) {
        var script = document.createElement("script");
        script.setAttribute('type', 'text/javascript');
        script.src = 'https://img.homepagemodules.de/ds/static/jquery/min/jquery-1.7.1.min.js';
        document.getElementsByTagName("head")[0].appendChild(script);

        colorbox();
        adjs();
    }
};

function colorbox() {
    var cbox = document.createElement("script");
    cbox.setAttribute('type', 'text/javascript');
    cbox.src = 'https://img.homepagemodules.de/ds/static/aktionen/adventskal/js/jquery.colorbox-min.js';
    document.getElementsByTagName("head")[0].appendChild(cbox);
}

function adjs() {
    var ajs = document.createElement("script");
    ajs.setAttribute('type', 'text/javascript');
    ajs.src = 'function placePicture(a){$("#"+a).html('<a id="'+a+'link" href="'+dayImgs[a-1]+'" class="group1" > '+a+"</a>")}function bbbaaabbb(a){var b=$("#"+a).css("position"),c=$("#"+a).css("display"),d=$("#"+a).css("width"),e=$("#"+a).length;"static"!=b&&(ssc(a),ects=6),"none"==c&&(ssc(a),ects=6),"300px"!=d&&(ssc(a),ects=6),$bxo.parent()[0]!==$("#containerCal")[0]&&($("#"+a).remove(),ssc(a),ects=6),e<1&&addxobad(),0!=ects&&(ects--,window.setTimeout("bbbaaabbb(aa)",1e3))}function ssc(a){$("#"+a).css("cssText","width: 300px !important;opacity: 1 !important ; float:left !important; border:1px solid #c7c7c7 !important; padding:3px !important; background-color: white !important; position: static !important; display: block !important; margin-top:-1.0%;")}function addxobad(){try{getHeightMargin()}catch(a){$("#containerCal").append('<div id="'+aa+'"></div>'),$("#"+aa).html('<img src="http://img.homepagemodules.de/xl16c.png" alt="Xobor.de"><a style="text-decoration:none; color: #8d8d8d;" href="http://www.xobor.de">&ensp;Einfach ein eigenes Xobor Forum erstellen</a>'),ssc(aa),$bxo=$("#innerCal ~ div").first(),window.setTimeout("bbbaaabbb(aa)",1e3)}}function snowSystem(){try{"true"==snow&&$(document).snowfall({flakeCount:maxFlakes,collection:".door",maxSpeed:maxFlakeSpeed,minSize:minFlakesSize,maxSize:mayFlakesSize,round:!0})}catch(a){}}function checkDateAndAction(a){dayImgs[a-1].match("http://")?$.colorbox({href:dayImgs[a-1]}):$.colorbox({iframe:!0,width:videoW+"px",height:videoH+"px",href:"http://www.youtube.com/embed/"+dayImgs[a-1]})}var tDate=new Date,today=tDate.getDate(),tMonth=tDate.getMonth()+offset,aa=Math.floor(1100500*Math.random()),dd=Math.floor(1100500*Math.random()),ects=6,$bxo;$(document).ready(function(){try{if("true"==snow){var a=document.createElement("script");a.setAttribute("type","text/javascript"),a.src="http://www.xobor.de/js_scripts/snow/snowfall.jquery.js",document.getElementsByTagName("head")[0].appendChild(a)}}catch(a){}if($("#mybg").attr("src",